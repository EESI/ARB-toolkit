lmenu:Import
menumeta:I
# 1 "MENUS/import.genmenu" 1


item:Create Import Filter from Metafile
seqtype:-
itemmethod:arb_import_pipeline.sh $ALIGN $TREE $METAFILE $METALABEL $OUTPUT_DIRECTORY 

arg:ALIGN
argtype:filename
arglabel: Aligned Fasta File

arg:TREE
argtype:filename
arglabel: Phylogenic Tree

arg:METAFILE
argtype:filename
arglabel: Metadata File

arg:METALABEL
argtype:filename
arglabel: Metadata sequence header column labels

arg:OUTPUT_DIRECTORY
argtype:filename
arglabel:Output folder location
out:OUTPUTFILE
outformat:flat
